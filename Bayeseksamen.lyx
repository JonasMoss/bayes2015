\begin_layout Subsection*
f)
\end_layout

\begin_layout Standard
Om 
\begin_inset Formula $(\theta_{L}^{1},\theta_{R}^{1}),(\theta_{L}^{2},\theta_{R}^{2}),...,(\theta_{L}^{N},\theta_{R}^{N})$
\end_inset

s er en trekning fra 
\begin_inset Formula $p(\theta_{L},\theta_{R}\mid\mathbf{y})$
\end_inset

 kan vi approksimere fordelingen til 
\begin_inset Formula $\frac{\theta_{L}}{\theta_{R}}\mid\mathbf{y}$
\end_inset

 med den empiriske fordelinga til 
\begin_inset Formula $\left(\frac{\theta_{L}^{i}}{\theta_{R}^{i}}\right)_{i=1}^{n}$
\end_inset

.
 Som et første steg bruker vi Metropolis-Hastings for å finne fordelingen
 
\begin_inset Formula $p(\theta_{L},\theta_{R},\tau\mid\mathbf{y})$
\end_inset

.
 Dette virker som et naturlig valg, ettersom de komplette betinga fordelingene
 ikke er spesielt pene.
 
\end_layout

\begin_layout Subsection*
g)
\end_layout

\begin_layout Standard
Vi reparametriserer slik at 
\begin_inset Formula $\gamma=\frac{\theta_{R}}{\theta_{L}}$
\end_inset

 og 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p(\gamma,\theta_{L}\tau,\mathbf{y})=\exp(-\gamma b)\gamma^{a-1}\frac{b^{a}}{n\prod_{i=1}^{n}y_{i}!\Gamma(a)}(\gamma\theta_{L})^{\sum_{i=\tau+1}^{n}y_{i}+a-1}\exp(-\theta_{L}\gamma(n-\tau+b))\theta_{L}^{\sum_{i=1}^{\tau}y_{i}+a-1}\exp(-\theta_{L}(\tau+b)).
\]

\end_inset

Nå kan vi integrere ut 
\begin_inset Formula $\theta_{L}$
\end_inset

 ved hjelp av observasjonen 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\exp(-\theta_{L}\gamma(n-\tau+b))\theta_{L}^{\sum_{i=1}^{n}y_{i}+a-2}\exp(-\theta_{L}(\tau+b) & =\\
\exp(-\theta_{L}(\gamma(n-\tau+b)+\tau+b))\theta_{L}^{\sum_{i=1}^{n}y_{i}+a-2},
\end{align*}

\end_inset

som er proporsjonal med 
\begin_inset Formula $\mbox{Gamma}(\gamma(n-\tau+b)+\tau+b),\sum_{i=1}^{n}y_{i}+a-1)$
\end_inset

.
 Altså blir
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p(\gamma,\tau,\mathbf{y})=\frac{b^{a}\Gamma(\sum_{i=1}^{n}y_{i}+a-1)\exp(-\gamma b)\gamma^{\sum_{i=\tau+1}^{n}y_{i}+2a-2}}{n\prod_{i=1}^{n}y_{i}!\Gamma(a)\left(\gamma(n-\tau+b)+\tau+b\right)^{\sum_{i=1}^{n}y_{i}+a-1}}.
\]

\end_inset

Å integrere vekk 
\begin_inset Formula $\tau$
\end_inset

 er bare å summere, så
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p(\gamma\mid\mathbf{y})\propto\sum_{\tau=1}^{n}\left[\frac{b^{a}\Gamma(\sum_{i=1}^{n}y_{i}+a-1)\exp(-\gamma b)\gamma^{\sum_{i=\tau+1}^{n}y_{i}+2a-2}}{n\prod_{i=1}^{n}y_{i}!\Gamma(a)\left(\gamma(n-\tau+b)+\tau+b\right)^{\sum_{i=1}^{n}y_{i}+a-1}}\right].
\]

\end_inset


\end_layout

\begin_layout Standard
Det er neppe lett å finne proporsjonalitetskonstanten 
\begin_inset Formula $p(\mathbf{y})$
\end_inset

 analytisk, men det bør gå numerisk.
\end_layout

